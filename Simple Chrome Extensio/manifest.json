{
  "name": "ATG extension",
  "description": "ATG Project",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs","storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    { 
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
